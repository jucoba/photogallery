<div class="w3-row-padding w3-margin-top">

<% Galleries.each do |id, gallery| %>
  <div class="w3-third">
    <div class="w3-card-2">
    <a href="<%= id %>">
      <img src='<%= gallery.url %>'
       style="width:100%" class="w3-hover-opacity" >
    </a>
    <div class="w3-container">
	     <h5><%= gallery.name %></h5>
    </div>
    </div>
  </div>
<% end %>

</div>





