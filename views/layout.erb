	<script>

function openImg(imageName) {
    var i;
    var x = document.getElementsByClassName("picture");
    for (i = 0; i < x.length; i++) {
       x[i].style.display = "none";
    }
    document.getElementById(imageName).style.display = "block";
}
</script>
